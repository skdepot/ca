<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="sale_return_stock_picking_form_view_inherited" model="ir.ui.view">
            <field name="name">stock.picking.form.view.inherited</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='picking_type_id']" position="after">
                    <field name="is_sale_return" invisible="1"/>
                    <field name="sale_return_id" invisible="1"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>